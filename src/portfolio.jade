						li
							img(src="img/portpolio/slider-img/project-indiacting-img2.png" alt="")

					.row 
						.col-sm-10.col-sm-offset-1
					
							ul.projcet-team.clearfix

								li.col-sm-6.projcet-team__member
									.card-img-wrapper
										img(src="img/portpolio/designer-profile-img.png" alt="")
									.card-view
										span.card-view__title project designer
										span.card-view__name  Smeer A
									.rectangular-border.rectangular-border-top

								li.col-sm-6.projcet-team__member
