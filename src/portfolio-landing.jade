<!DOCTYPE html>
head
	meta(charset="UTF-8")
	meta(name="viewport", content="width=device-width, initial-scale=1")

	title Bonito - Portfolio-Landing

	link(rel="stylesheet", href="css/main.css")
body

	main
		//navbar
		include jade-components/base/header-nav
		
		//main-video section

		section.portfolio-all-projects
			.container
				.row 
					.col-sm-12.section-intro-left
						include  jade-components/comman/section-intro.jade
						+section-intro("All Projects", "Interior designing is not just to impress your guests but also to ensure that when you’re away from your beautiful home, it makes you want to come back. That’s what we do.")

					.col-sm-12.portfolio-filters
						.portfolio-filters__heading 
								span.portfolio-filters__heading-info filters
						.portfolio-filters__info.filters
							.filters__heading
								.fa.fa-close.filters__close-btn
								.filters__close-heading filters	
							.filters__content
								#accordion.panel-group
									.panel.panel-default
										.panel-heading
											h3.panel-title
												a(data-toggle='collapse', data-parent='#accordion', href='#collapse1') Collapsible Group 1
												i.indicator.fa.fa-minus
										#collapse1.panel-collapse.collapse.in
											.panel-body
												.col-sm-6.col-md-4
													label 
														input(type="checkbox" name="apartment-name")
														span.check-box apartment-name

									.panel.panel-default
										.panel-heading
											h3.panel-title
												a(data-toggle='collapse', data-parent='#accordion', href='#collapse2') Collapsible Group 1
												i.indicator.fa.fa-plus
										#collapse2.panel-collapse.collapse
											.panel-body
												p second tab

									.panel.panel-default
										.panel-heading
											h3.panel-title
												a(data-toggle='collapse', data-parent='#accordion', href='#collapse3') Collapsible Group 1
												i.indicator.fa.fa-plus
										#collapse3.panel-collapse.collapse
											.panel-body
												p third tab

		section.about-us-video-section.portfolio-video-section
			.container
				.row
					.about-us-video-section-container.clearfix
						.col-sm-8
							.about-us-video-banner__wraper
								img.about-us-video-banner-img(src="img/about-us-video-banner.png" alt="about-video-baner")
								.rectangular-border-bottom-right
								a.about-us-video-banner-icon.video#open-popup
									i.fa.fa-play 

						.col-sm-4.clearfix
							.about-us-video-content.portfolio-video
								.portfolio-video__heading Mr. Veeresh’s House Interiors Design
								.portfolio-video__sub-heading Nitesh Columbus Square Apartment
								p.portfolio-video__info A walk through the Nitesh Columbus square apartment of Mr.Veeresh


								.portfolio-video__design
									img.portfolio-video__img(src="img/bonito-subscribe.png" alt="subscribe image")
									.subscribe-wrapper
										.portfolio-video__design-heading Bonito Designs
										a.portfolio-video__subsribe.subscribe
											.subscribe__content
												span.subscribe__icon.fa.fa-youtube-play
												span.subscribe__info Subscribe
											.subscribe__number 3,245 
	

